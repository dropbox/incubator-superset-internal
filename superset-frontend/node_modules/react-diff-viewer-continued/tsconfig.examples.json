{
  "compilerOptions": {
    "experimentalDecorators": true,
    "jsx": "react-jsx",
    "module": "es6",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "target": "es5",
    "downlevelIteration": true,
    "lib": ["es2017", "dom"]
  },
  "exclude": ["node_modules"]
}
