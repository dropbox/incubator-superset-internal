{
  "name": "react-diff-viewer-continued",
  "version": "3.2.5",
  "private": false,
  "description": "Continuation of a simple and beautiful text diff viewer component made with diff and React",
  "keywords": [
    "review",
    "code-review",
    "diff",
    "diff-viewer",
    "github",
    "react",
    "react-component",
    "ui"
  ],
  "repository": "git@github.com:aeolun/react-diff-viewer-continued.git",
