{
  "parser": "babel-eslint",
  "extends": [
    "eslint-config-airbnb",
  ],
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-console": [0],
    "func-names": [0],
    "semi": [2, "never"],
    "no-extra-semi": [2],
    "react/prefer-es6-class": [0],
    "react/prefer-stateless-function": [0],
  }
}
